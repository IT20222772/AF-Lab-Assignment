"sources":{"Component/ViewPromotion.js":"import React, { Component } from \"react\";\r\nimport axios from \"axios\";\r\n\r\nexport default class Promotion extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n\r\n    this.state = {\r\n      promotion: [],\r\n    };\r\n  }\r\n\r\n  componentDidMount() {\r\n    this.retrivePosts();\r\n  }\r\n\r\n  retrivePosts() {\r\n    axios.get(\"http://localhost:3000/promotion/getpromotion\").then((res) => {\r\n      if (res.data.success) {\r\n        this.setState({\r\n          promotion: res.data.exsitingpromotion,\r\n        });\r\n        console.log(this.state.promotion);\r\n      }\r\n    });\r\n  }\r\n \r\n\r\n\r\n  render() {\r\n    return (\r\n      <div >\r\n        <br />\r\n        <center>\r\n            <h1 style={{color:\"blue\"}}>Promotions</h1>\r\n        <table\r\n          style={{ border:\"2px solid black\", width:\"400px\" }}\r\n        >\r\n          \r\n          <tbody>\r\n            {this.state.promotion.map((promotion, index) => (\r\n              <tr>\r\n                <th scope=\"row\"><h1>{index + 1}</h1></th>\r\n               \r\n                <tr>Product : {promotion.name}</tr>\r\n                <tr>Price : Rs.{promotion.price}</tr>\r\n                <tr style={{color:\"red\"}}>Discount : {promotion.discount}%</tr>\r\n                <tr>Discount Price : Rs.{promotion.price-promotion.price*(promotion.discount/100)}</tr>\r\n                <br/>\r\n              </tr>\r\n             \r\n            ))}\r\n          </tbody>\r\n        </table>\r\n        <br/>\r\n        </center>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d20a99879962aa5a14734e6bd247b265","cacheData":{"env":{}}}