t/ViewItem.js","original":{"line":86,"column":3}},{"generated":{"line":133,"column":2},"source":"Component/ViewItem.js","original":{"line":5,"column":37},"name":"Component"}],"sources":{"Component/ViewItem.js":"import React, { Component } from \"react\";\r\nimport axios from \"axios\";\r\nimport {useLocation, useMatch, useNavigate, withRouter} from 'react-router-dom'\r\n\r\nexport default class GetMark extends Component {\r\n  \r\n  constructor(props) {\r\n    super(props);\r\n\r\n    this.state = {\r\n      Items: [],\r\n    };\r\n  }\r\n\r\n  componentDidMount() {\r\n    this.retrivePosts();\r\n  }\r\n\r\n  retrivePosts() {\r\n    axios.get(\"http://localhost:8000/Items/getall\").then((res) => {\r\n      if (res.data.success) {\r\n        this.setState({\r\n          Items: res.data.exsitingItems,\r\n        });\r\n        console.log(this.state.Items);\r\n      }\r\n    });\r\n  }\r\n  onDelete = (id) => {\r\n    axios.delete(`http://localhost:3000/Items/itemdelete/${id}`).then((res) => {\r\n      alert(\"Delete Successfully\");\r\n      this.retrivePosts();\r\n    });\r\n  };\r\n  \r\n\r\n  render() {\r\n    return (\r\n      <div >\r\n        <br />\r\n        <center>\r\n        <table\r\n          className=\"table\"\r\n          style={{ background: \"Gray\", width: \"1200px\", color:\"white\"}}\r\n        >\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\"> Item ID</th>\r\n              <th scope=\"col\"> Name</th>\r\n              <th scope=\"col\">Quentity</th>\r\n              <th scope=\"col\">Price</th>\r\n              <th scope=\"col\">Type</th>\r\n             \r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            {this.state.Items.map((Items, index) => (\r\n              <tr>\r\n                <th scope=\"row\">{index + 1}</th>\r\n                <td>{Items.id}</td>\r\n                <td>{Items.name}</td>\r\n                <td>{Items.quentity}</td>\r\n                <td>{Items.price}</td>\r\n                <td>{Items.type}</td>\r\n                <button\r\n                    type=\"button\"\r\n                    className=\"btn btn-warning\"\r\n                    onClick={() => this.onDelete(Items.id)}\r\n                  > Delete</button>\r\n\r\n                 <button\r\n                    type=\"button\"\r\n                    className=\"btn btn-warning\"\r\n                    onClick={()=>{navigate('/updateitem')}}\r\n                  > Update</button>\r\n\r\n\r\n                \r\n              </tr>\r\n            ))}\r\n          </tbody>\r\n        </table>\r\n        </center>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"18ec88c20a5dfa931f218ef2d1945123","cacheData":{"env":{}}}